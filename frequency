#include <stdio.h>

int main() {
    int a[50], n, i, j, count;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    for(i = 0; i < n; i++) {
        count = 1;

        // Check if already counted
        for(j = 0; j < i; j++) {
            if(a[i] == a[j])
                break;
        }

        // If already appeared, skip
        if(j != i)
            continue;

        // Count frequency
        for(j = i + 1; j < n; j++) {
            if(a[i] == a[j])
                count++;
        }

        printf("%d occurs %d times\n", a[i], count);
    }

    return 0;
}
