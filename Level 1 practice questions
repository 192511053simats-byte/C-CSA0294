26.	Rhombus Star Pattern in C
*
***
*****
***
*
Ans:
#include <stdio.h>
int main()
{
	int n,i,j,k;
	char c;
	
	printf("Enter the no of rows");
	scanf("%d",&n);
	printf("Enter the Symbol");
	scanf(" %c",&c);
	
	for(i=1;i<=n;i++)
	{
		
		for(k=1;k<=(2*i-1);k++)
		{
			printf("%c",c);
		}
		printf("\n");
	}
	
	for(i=n-1;i>=1;i--)
	{
		for(k=1;k<=(2*i-1);k++)
		{
			printf("%c",c);
		}
		printf("\n");
	}
	
}
------------------------------------------------------------------------------------------------------------------------------------------------------------
27.	Diamond Star Pattern
Enter the number of rows: 5
    *
   ***
  *****
   ***
    *
Ans:
#include <stdio.h>
int main()
{
	int n,i,j,k;
	char c;
	
	printf("Enter the no of rows");
	scanf("%d",&n);
	printf("Enter the Symbol");
	scanf(" %c",&c);
	
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=n-i;j++)
		{
		   printf(" ");
		}
		for(k=1;k<=(2*i-1);k++)
		{
			printf("%c",c);
		}
		printf("\n");
	}
	
	for(i=n-1;i>=1;i--)
	{
		for(j=1;j<=n-i;j++)
		{
		   printf(" ");
		}
		for(k=1;k<=(2*i-1);k++)
		{
			printf("%c",c);
		}
		printf("\n");
	}
	
}
