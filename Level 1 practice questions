int main()
{
	int a,b,t,s,p;
	printf("Enter the number 1");
	scanf("%d",&a);
	printf("Enter the number 2");
	scanf("%d",&b);
	s=a;
	p=b;
		
	
	while(b!=0)
	{
		t=b;
		b=a%b;
		a=t;
		
		
	}
	printf("gcd %d\n",a);
}
