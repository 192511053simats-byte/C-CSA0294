#include <stdio.h>

int main()
{
    int n, i, j, k;
    float a[20][40], ratio;

    scanf("%d", &n);

    // Input matrix
    for(i = 0; i < n; i++)
    {
        for(j = 0; j < n; j++)
        {
            scanf("%f", &a[i][j]);
        }
    }

    // Add identity matrix
    for(i = 0; i < n; i++)
    {
        for(j = n; j < 2*n; j++)
        {
            if(i == j - n)
                a[i][j] = 1;
            else
                a[i][j] = 0;
        }
    }

    // Gauss Jordan process
    for(i = 0; i < n; i++)
    {
        if(a[i][i] == 0)
        {
            printf("Inverse not possible\n");
            return 0;
        }

        for(j = 0; j < n; j++)
        {
            if(i != j)
            {
                ratio = a[j][i] / a[i][i];

                for(k = 0; k < 2*n; k++)
                {
                    a[j][k] = a[j][k] - ratio * a[i][k];
                }
            }
        }
    }

    // Make diagonal elements = 1
    for(i = 0; i < n; i++)
    {
        for(j = n; j < 2*n; j++)
        {
            a[i][j] = a[i][j] / a[i][i];
        }
    }

    // Print inverse matrix
    for(i = 0; i < n; i++)
    {
        for(j = n; j < 2*n; j++)
        {
            printf("%.2f ", a[i][j]);
        }
        printf("\n");
    }

    return 0;
}
