#include <stdio.h>
int main()
{
	int i,j,k,n,a[50],t,s,count=0;
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	scanf("%d",&t);
	
	for(i=0;i<n;i++)
	{
		for(j=i+1;j<n;j++)
		{
			for(k=j+1;k<n;k++)
			{
				s=a[i]+a[j]+a[k];
				if(s==t)
				{
						printf("[%d,%d,%d] ",a[i],a[j],a[k]);
				}
			
				
			}	
		}
	}

	
	
	
	
}
